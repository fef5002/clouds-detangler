{
  "scan_paths": {
    "google_drive": [
      "~/Google Drive"
    ],
    "onedrive": [
      "~/OneDrive"
    ],
    "icloud": [
      "~/Library/Mobile Documents/com~apple~CloudDocs"
    ],
    "dropbox": [
      "~/Dropbox"
    ]
  },
  "exclude_patterns": [
    "*.tmp",
    "*.cache",
    ".DS_Store",
    "Thumbs.db"
  ],
  "min_file_size": 0,
  "output_directory": ".",
  "warnings": {
    "show_sync_warning": true,
    "require_confirmation": true
  }
}
